<span>年度: </span>
<div class="btn-group">
    <a href="#" class="btn dropdown-toggle" id="dropYear" data-toggle="dropdown">
        <span id="dropYearLabel"><%= currentYear %>年度</span>
        <span class="caret"></span>
    </a>
    <ul id="dropYearMenu" class="dropdown-menu" role="menu" aria-labelledby="dropYear">
    <% years.forEach(function(y){ %>
        <li><a href="#" data-value="<%= y %>"><%= y %>年度</a></li>
    <% }); %>
    </ul>
</div>

